clientPort={{ zk_clientPort }}
tickTime={{ zk_tickTime }}
initLimit={{ zk_initLimit }}
syncLimit={{ zk_syncLimit }}
dataDir={{ zk_dataDir }}
logDir="{{ zk_home_dir }}/{{ zk_user }}/logs"

{% for server in groups['zookeeper'] %}
server.{{ hostvars[id] }}={{ hostvars[server] }}:{{ zk_clusterPorts }}
{% endfor %}
